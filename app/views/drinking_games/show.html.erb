<div class="page-header">
  <%= link_to drinking_games_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_drinking_game_path(@drinking_game), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show drinking_game</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @drinking_game.name %></dd>

  <dt>Number of people:</dt>
  <dd><%= @drinking_game.number_of_people %></dd>

  <dt>Supplies:</dt>
  <dd><%= @drinking_game.supplies %></dd>

</dl>
